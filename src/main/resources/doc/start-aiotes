                ]>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns="http://www.inter-iot.eu/sripas#node_" xmlns:xsd="http://www.w3.org/2001/XMLSchema#" xmlns:medex="http://inter-iot.eu/medex#" xmlns:time="http://www.w3.org/2006/time#" xmlns:sweet_units="http://sweet.jpl.nasa.gov/2.3/reprSciUnits.owl#" xmlns:ssn="http://www.w3.org/ns/ssn/" xmlns:aha="http://ontologies.activageproject.eu/aha-ontology#" xmlns:m3="http://purl.org/iot/vocab/m3-lite" xmlns:iiot="http://inter-iot.eu/GOIoTP#"  xmlns:iiotex="http://inter-iot.eu/GOIoTPex#"  xmlns:exmo="http://exmo.inrialpes.fr/align/ext/1.0/#"  xmlns:sripas="http://www.inter-iot.eu/sripas#"  xmlns:dcelem="http://purl.org/dc/elements/1.1/"  xmlns:var="http://www.inter-iot.eu/sripas#node_"  xmlns:align="http://knowledgeweb.semanticweb.org/heterogeneity/alignment#"  xmlns:sosa="http://www.w3.org/ns/sosa/" xmlns:sensinact="http://sensinact.com#">

    <align:Alignment>
        <dcelem:title>SensiNact2AIOTES</dcelem:title>
        <exmo:version>0.1</exmo:version>
        <dcelem:creator>CEA/UPV</dcelem:creator>
        <dcelem:description>Upstream Alignment from SensiNact ontology to ACTIVAGE ontology.</dcelem:description>
        <dcelem:date>18 July 2019</dcelem:date>
    <align:onto1>
      <align:Ontology rdf:about="http://sensinact.com#">
        <align:formalism>
          <align:Formalism align:name="OWL2.0" align:uri="http://www.w3.org/2002/07/owl#"/>
        </align:formalism>
      </align:Ontology>
        </align:onto1>
    <align:onto2>
      <align:Ontology rdf:about="http://inter-iot.eu/GOIoTPex#">
        <align:formalism>
          <align:Formalism align:name="OWL2.0" align:uri="http://www.w3.org/2002/07/owl#"/>
        </align:formalism>
      </align:Ontology>
        </align:onto2>
    <sripas:steps rdf:parseType="Literal">
      <sripas:step sripas:order="1" sripas:cell="http://www.inter-iot.eu/sripas#1_provider"/>
       
      <sripas:step sripas:order="2" sripas:cell="http://www.inter-iot.eu/sripas#2_observation"/>
       
      <sripas:step sripas:order="3" sripas:cell="http://www.inter-iot.eu/sripas#3_resource"/>
       
      <sripas:step sripas:order="4" sripas:cell="http://www.inter-iot.eu/sripas#4_clean"/>
        </sripas:steps>
		
		
        <sripas:cellFormat>
            <iiot:DataFormat rdf:about="http://inter-iot.eu/sripas#rdfxml"/>
        </sripas:cellFormat>
		
        <align:map>
      <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_provider">
        <align:entity1 rdf:parseType="Literal">
          <var:provider>
            <rdf:type rdf:resource="http://sensinact.com#Provider"/>
             
                        <sensinact:name>
                            <var:provider_name/>
            </sensinact:name>
             
            <sensinact:manages>
              <var:service>
                <rdf:type rdf:resource="http://sensinact.com#Service"/>
                 
                                <sensinact:name>
                                    <var:service_name/>
                </sensinact:name>
              </var:service>
            </sensinact:manages>
          </var:provider>
        </align:entity1>
         
        <align:entity2 rdf:parseType="Literal">
          <var:provider>
            <rdf:type rdf:resource="http://www.w3.org/ns/sosa/IoTDevice"/>
             
            <rdf:type rdf:resource="http://www.w3.org/ns/sosa/Platform"/>
             
                        <iiot:hasName>
                            <var:provider_name/>
            </iiot:hasName>
             
            <sosa:hosts>
              <var:service>
                <rdf:type rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
                 
