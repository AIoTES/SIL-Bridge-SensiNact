            </align:Cell>
        </align:map>
        <align:map>
      <align:Cell rdf:about="http://www.inter-iot.eu/sripas#2_observation">
        <align:entity1 rdf:parseType="Literal">
          <var:sensor>
            <rdf:type rdf:resource="http://www.w3.org/ns/sosa/Sensor"/>
             
