<?xml version="1.0" encoding="UTF-8"?>
    <!DOCTYPE RDF>
    <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:sripas="http://www.inter-iot.eu/sripas#" 
         xmlns:var="http://www.inter-iot.eu/sripas#node_" 
         xmlns:align="http://knowledgeweb.semanticweb.org/heterogeneity/alignment#" 
         xmlns:dcelem="http://purl.org/dc/elements/1.1/" 
         xmlns:exmo="http://exmo.inrialpes.fr/align/ext/1.0/#" 
         xmlns="http://www.inter-iot.eu/sripas#" 
         xmlns:sosa="http://www.w3.org/ns/sosa/" 
         xmlns:iiot="http://inter-iot.eu/GOIoTP#" 
         xmlns:iiotex="http://inter-iot.eu/GOIoTPex#" 
         xmlns:time="http://www.w3.org/2006/time#" 
         xmlns:sweet_units="http://sweet.jpl.nasa.gov/2.3/reprSciUnits.owl#" 
         xmlns:medex="http://inter-iot.eu/medex#" 
         xmlns:ssn="http://www.w3.org/ns/ssn/" 
         xmlns:aha="http://ontologies.activageproject.eu/aha-ontology#" 
         xmlns:m3="http://purl.org/iot/vocab/m3-lite" 
         xmlns:fw="http://inter-iot.eu/syntax/FIWAREv2#" 
         xmlns:geosparql="http://www.opengis.net/ont/geosparql#" 
         xmlns:sensinact="http://sensinact.com#">    
      
    <align:Alignment>
        <dcelem:title>SensiNact2AIoTES</dcelem:title>
        <exmo:version>0.2.6</exmo:version>
        <dcelem:creator>CEA/UPV</dcelem:creator>
        <dcelem:description>Alignment between SensiNact ontology and ACTIVAGE ontology.</dcelem:description>
        <dcelem:date>18 July 2019</dcelem:date>
        
        <align:onto1>
            <align:Ontology rdf:about="http://sensinact.com#">
                <align:formalism>
                    <align:Formalism align:name="OWL2.0" align:uri="http://www.w3.org/2002/07/owl#"/>
                </align:formalism>
            </align:Ontology>
        </align:onto1>
        
        <align:onto2>
            <align:Ontology rdf:about="http://aha.ontology.com/ACTIVAGE#">
                <align:formalism>
                    <align:Formalism align:name="OWL2.0" align:uri="http://www.w3.org/2002/07/owl#"/>
                </align:formalism>
            </align:Ontology>
        </align:onto2>
        
        <sripas:steps rdf:parseType="Literal">
            <sripas:step sripas:order="1" sripas:cell="http://www.inter-iot.eu/sripas#1_provider"/>
          
            <sripas:step sripas:order="2" sripas:cell="http://www.inter-iot.eu/sripas#2_observation"/>
          
            <sripas:step sripas:order="3" sripas:cell="http://www.inter-iot.eu/sripas#3_resource"/>
          
            <sripas:step sripas:order="4" sripas:cell="http://www.inter-iot.eu/sripas#4_clean"/>
          	 
            <sripas:step sripas:order="5" sripas:cell="http://www.inter-iot.eu/sripas#5_bedsensor"/>
          	 
            <sripas:step sripas:order="6" sripas:cell="http://www.inter-iot.eu/sripas#6_eliminate_useless_classes"/>
          	 
        </sripas:steps>
        		
        		
        <sripas:cellFormat>
            <iiot:DataFormat rdf:about="http://inter-iot.eu/sripas#turtle"/>
        </sripas:cellFormat>
        		
        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#1_provider">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a                  sensinact:Provider , sripas:node_provider ;
                    sensinact:manages  [ a               sensinact:Service , sripas:node_service ;
                    sensinact:name  [ a  sripas:node_service_name ]
                    ] ;
                    sensinact:name     [ a  sripas:node_provider_name ]
                    ] .
                </align:entity1>
            
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a             sosa:Platform , sosa:IoTDevice , sripas:node_provider ;
                    iiot:hasName  [ a  sripas:node_provider_name ] ;
                    sosa:hosts    [ a             sosa:Sensor , sripas:node_service ;
                    iiot:hasName  [ a  sripas:node_service_name ]
                    ]
                    ] .
                </align:entity2>
            
                <align:relation>=</align:relation>
            </align:Cell>
        </align:map>
        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#2_observation">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a                  sosa:Sensor , sripas:node_sensor ;
                    sensinact:manages  [ a                sensinact:Resource , sripas:node_resource ;
                    sensinact:name   [ a  sripas:node_resource_name ] ;
                    sensinact:value  [ a  sripas:node_resource_value ]
                    ]
                    ] .
                </align:entity1>
            
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a                        sosa:Observation ;
                    sosa:ObservableProperty  [ a                ssn:Property , rdf:description ;
                    ssn:hasProperty  "occupied"
                    ] ;
                    sosa:hasResult           [ a                    sosa:Result ;
                    iiot:hasName         [ a  sripas:node_resource_name ] ;
                    iiot:hasResultValue  [ a  sripas:node_resource_value ]
                    ] ;
                    sosa:madeBySensor        [ a             sosa:Sensor , sripas:node_sensor ;
                    sosa:oberves  [ a  sripas:node_resource ]
                    ] ;
                    sosa:observedProperty    [ a  sripas:node_resource ] ;
                    sosa:phenomenomTime      [ a                    time:Instant , rdf:description ;
                    time:inTimePosition  "1562880899012"
                    ]
                    ] .
                </align:entity2>
            
                <align:relation>=</align:relation>
            </align:Cell>
        </align:map>
        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#3_resource">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a                  sosa:Sensor , sripas:node_sensor ;
                    sensinact:manages  [ a  sensinact:Resource , sripas:node_resource ]
                    ] .
                </align:entity1>
            
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a              sosa:Sensor , sripas:node_sensor ;
                    sosa:observes  [ a  sripas:node_resource ]
                    ] .
                </align:entity2>
            
                <align:relation>=</align:relation>
            </align:Cell>
        </align:map>
        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#4_clean">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a                  sosa:Sensor , sripas:node_sensor ;
                    sensinact:manages  [ a  sensinact:Resource , sripas:node_resource ]
                    ] .
                </align:entity1>
            
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a       sosa:Sensor , sripas:node_sensor ] .
                </align:entity2>
            
                <align:relation>=</align:relation>
            </align:Cell>
        </align:map>
        		
        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#5_bedsensor">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a       sosa:Sensor , sripas:node_sensor ] .
                </align:entity1>
            
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a                &lt;http://ontology.universAAL.org/PhThing.owl#Device&gt; , &lt;http://ontology.universAAL.org/PhThing.owl#PhysicalThing&gt; , m3:PressureSensor , sosa:Sensor , sripas:node_sensor ;
                    sosa:isHostedBy  "originalPlatformSensiNact"
                    ] .
                </align:entity2>
            
                <align:relation>=</align:relation>
            </align:Cell>
        </align:map>
        <align:map>
            <align:Cell rdf:about="http://www.inter-iot.eu/sripas#6_eliminate_useless_classes">
                <align:entity1 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a                  sosa:Sensor , sripas:node_sensor ;
                    sensinact:manages  [ a  sensinact:Resource , sripas:node_resource ]
                    ] .
                </align:entity1>
            
                <align:entity2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
                    [ a       sosa:Sensor , sripas:node_sensor ] .
                </align:entity2>
            
                <align:relation>=</align:relation>
            </align:Cell>
        </align:map>      		
      	
    </align:Alignment>
</rdf:RDF>