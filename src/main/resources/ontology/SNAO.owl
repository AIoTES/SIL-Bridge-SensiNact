@prefix : <http://sensinact.com#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://sensinact.com> .

<http://sensinact.com> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://sensinact.com#contains
:contains rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://sensinact.com#isManagedBy
:isManagedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             owl:inverseOf :manages ;
             rdf:type owl:FunctionalProperty ,
                      owl:InverseFunctionalProperty .


###  http://sensinact.com#manages
:manages rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ,
                  owl:InverseFunctionalProperty .


#################################################################
#    Data properties
#################################################################

###  http://sensinact.com#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:domain [ rdf:type owl:Restriction ;
                    owl:onProperty :name ;
                    owl:allValuesFrom xsd:string
                  ] .


#################################################################
#    Classes
#################################################################

###  http://sensinact.com#Element
:Element rdf:type owl:Class ;
         rdfs:comment :name .


###  http://sensinact.com#Provider
:Provider rdf:type owl:Class ;
          rdfs:subClassOf :Element ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :manages ;
                            owl:allValuesFrom :Service
                          ] .


###  http://sensinact.com#Resource
:Resource rdf:type owl:Class ;
          rdfs:subClassOf :Element ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :isManagedBy ;
                            owl:allValuesFrom :Service
                          ] .


###  http://sensinact.com#Service
:Service rdf:type owl:Class ;
         rdfs:subClassOf :Element ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :isManagedBy ;
                           owl:allValuesFrom :Provider
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :manages ;
                           owl:allValuesFrom :Resource
                         ] .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Provider
                :Resource
                :Service
              )
] .


